# Authentication URL and service account credentials - mandatory
AAS_BASE_URL=https://isecl-aas:8444/aas/v1

# CMS URL and CMS webserivce TLS hash for server verification - mandatory
#Required for generating TLS certificate
CMS_BASE_URL=https://isecl-cms:8445/cms/v1
APS_BASE_URL=https://isecl-aps:5443/aps/v1
CMS_TLS_CERT_SHA384=sha384valueofCMSTLSCert

# Installation admin bearer token for CSR approval request to CMS - mandatory
# JWT token for AAS superuser
BEARER_TOKEN=<Bearer Token>

#Custom token to access APS
CUSTOM_TOKEN=<Custom Token>

# URL endpoints of KBS server
ENDPOINT_URL=<URL of KBS>

#Sets the root log level in config.yml
LOG_LEVEL=INFO

#Skips setup during installation if set to true
KBS_NOSETUP=false

#Key manager to be used for key storage. By default, this environment variable shall be set to KMIP.
KEY_MANAGER=KMIP

KMIP_SERVER_IP=
KMIP_SERVER_PORT=
KMIP_CLIENT_CERT_PATH=
KMIP_CLIENT_KEY_PATH=
KMIP_ROOT_CERT_PATH=

